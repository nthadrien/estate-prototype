<div id="carouselPatnersCaptions" class="carousel slide" data-bs-ride="carousel">

    <div class="carousel-indicators">
      <button type="button" data-bs-target="#carouselPatnersCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
      <button type="button" data-bs-target="#carouselPatnersCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
      <button type="button" data-bs-target="#carouselPatnersCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>

    <div style="min-height: 140px;" class="carousel-inner">

      <aside class="carousel-item active"  data-bs-interval="3000">
        <div class="text-secondary text-center">
          <h5>First slide label</h5>
          <p>Some representative placeholder content for the first slide.</p>
        </div>
      </aside>

      <aside class="carousel-item"  data-bs-interval="6000">
        <div class="text-secondary text-center">
          <h5>Second slide label</h5>
          <p>Some representative placeholder content for the second slide.</p>
        </div>
      </aside>

      <aside class="carousel-item"  data-bs-interval="9000">
        <div class="text-muted text-center">
          <h5>Third slide label</h5>
          <p>Some representative placeholder content for the third slide.</p>
        </div>
      </aside>

    </div>

    <button class="carousel-control-prev" type="button" data-bs-target="#carouselPatnersCaptions" data-bs-slide="prev">
        <i class="bi bi-chevron-compact-left text-warning h2"></i>
    </button>

    <button class="carousel-control-next" type="button" data-bs-target="#carouselPatnersCaptions" data-bs-slide="next">
      <i class="bi bi-chevron-compact-right text-warning h2"></i>
    </button>
